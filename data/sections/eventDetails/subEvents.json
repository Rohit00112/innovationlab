{
  "tabId": "sub-events",
  "containerClasses": "content-section mb-16 bg-white rounded-xl p-4 md:p-8 shadow-sm border border-gray-100",
  "header": {
    "containerClasses": "flex flex-col items-center mb-10 text-center",
    "icon": {
      "containerClasses": "w-16 h-16 rounded-full bg-[#0066FF]/10 flex items-center justify-center mb-4 sub-events-icon",
      "classes": "w-8 h-8 text-[#0066FF]",
      "viewBox": "0 0 24 24",
      "path": {
        "d": "M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z",
        "strokeLinecap": "round",
        "strokeLinejoin": "round",
        "strokeWidth": "2"
      }
    },
    "title": {
      "text": "Sub-Events",
      "classes": "text-3xl font-bold text-gray-900 mb-3 sub-events-title"
    },
    "divider": {
      "classes": "w-24 h-1 bg-[#0066FF] rounded-full mb-4 sub-events-divider"
    },
    "description": {
      "text": "Explore the various activities and sessions that make up our comprehensive event program. Each sub-event offers unique opportunities for learning, networking, and showcasing your skills.",
      "classes": "text-gray-600 max-w-3xl sub-events-description"
    }
  },
  "grid": {
    "containerClasses": "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-2 gap-4 md:gap-8 mt-6 md:mt-10 sub-events-grid",
    "card": {
      "containerClasses": "bg-white rounded-xl overflow-hidden shadow-md border border-gray-100 transition-all duration-300 hover:shadow-lg sub-event-card",
      "imageContainer": {
        "classes": "relative h-48 overflow-hidden"
      },
      "image": {
        "classes": "object-cover transition-transform duration-500 group-hover:scale-105",
        "sizes": "(max-width: 768px) 100vw, (max-width: 1200px) 50vw, 33vw"
      },
      "category": {
        "containerClasses": "absolute top-4 left-4 px-3 py-1 rounded-full text-xs font-semibold text-white shadow-sm sub-event-category",
        "colorClasses": {
          "Career": "bg-[#0066FF]",
          "Innovation": "bg-[#5045E8]",
          "Coding": "bg-[#EEAE22]",
          "Learning": "bg-[#00C853]"
        }
      },
      "content": {
        "containerClasses": "p-6",
        "title": {
          "classes": "text-xl font-bold text-gray-900 mb-2 sub-event-title"
        },
        "description": {
          "classes": "text-gray-600 mb-4 sub-event-description"
        },
        "details": {
          "containerClasses": "flex flex-col space-y-2 mb-4",
          "dateTime": {
            "containerClasses": "flex items-center text-sm text-gray-500",
            "icon": {
              "classes": "w-4 h-4 mr-2 text-gray-400",
              "viewBox": "0 0 24 24",
              "path": {
                "d": "M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z",
                "strokeLinecap": "round",
                "strokeLinejoin": "round",
                "strokeWidth": "2"
              }
            }
          },
          "location": {
            "containerClasses": "flex items-center text-sm text-gray-500",
            "icon": {
              "classes": "w-4 h-4 mr-2 text-gray-400",
              "viewBox": "0 0 24 24",
              "path": {
                "d": "M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z",
                "strokeLinecap": "round",
                "strokeLinejoin": "round",
                "strokeWidth": "2"
              }
            }
          }
        },
        "link": {
          "containerClasses": "flex items-center text-[#0066FF] font-medium hover:text-[#0055DD] transition-colors duration-300 sub-event-link",
          "text": "Learn More",
          "icon": {
            "classes": "w-4 h-4 ml-1",
            "viewBox": "0 0 24 24",
            "path": {
              "d": "M13 7l5 5m0 0l-5 5m5-5H6",
              "strokeLinecap": "round",
              "strokeLinejoin": "round",
              "strokeWidth": "2"
            }
          }
        }
      }
    }
  },
  "cta": {
    "containerClasses": "mt-10 md:mt-16 bg-gradient-to-br from-[#0066FF] to-[#5045E8] rounded-xl p-6 md:p-8 text-white text-center relative overflow-hidden sub-events-cta",
    "decorativeElements": [
      {
        "classes": "absolute top-0 right-0 w-64 h-64 bg-white/10 rounded-full -mr-32 -mt-32 decorative-element-1"
      },
      {
        "classes": "absolute bottom-0 left-0 w-64 h-64 bg-white/10 rounded-full -ml-32 -mb-32 decorative-element-2"
      }
    ],
    "content": {
      "containerClasses": "relative z-10 max-w-2xl mx-auto",
      "title": {
        "text": "Ready to Join the Innovation Challenge?",
        "classes": "text-2xl md:text-3xl font-bold mb-4 cta-title"
      },
      "description": {
        "text": "Register now to secure your spot in this exciting event. Limited seats available for each sub-event.",
        "classes": "text-white/90 mb-8 cta-description"
      },
      "buttons": {
        "containerClasses": "flex flex-col sm:flex-row flex-wrap justify-center gap-3 md:gap-4",
        "register": {
          "href": "https://docs.google.com/forms/d/e/1FAIpQLSfDn9Evo7cRpeGIe45MWdvbYKRB3rj8wpVjNGV5FwwFnBGPRg/viewform",
          "classes": "bg-white text-[#0066FF] font-medium px-6 py-3 rounded-full shadow-md hover:shadow-lg transition-all duration-300 register-cta-btn",
          "text": "Register Now",
          "target": "_blank",
          "rel": "noopener noreferrer"
        },
        "schedule": {
          "href": "#agenda",
          "classes": "bg-transparent border-2 border-white text-white font-medium px-6 py-3 rounded-full hover:bg-white/10 transition-all duration-300 schedule-cta-btn",
          "text": "View Full Schedule"
        }
      }
    }
  },
  "animations": {
    "header": {
      "icon": {
        "y": 20,
        "opacity": 0,
        "duration": 0.7,
        "delay": 0.3,
        "ease": "power3.out"
      },
      "title": {
        "y": 20,
        "opacity": 0,
        "duration": 0.7,
        "delay": 0.4,
        "ease": "power2.out"
      },
      "divider": {
        "width": 0,
        "opacity": 0,
        "duration": 0.7,
        "delay": 0.5,
        "ease": "power1.inOut"
      },
      "description": {
        "y": 20,
        "opacity": 0,
        "duration": 0.7,
        "delay": 0.6,
        "ease": "power2.out"
      }
    },
    "cards": {
      "container": {
        "y": 30,
        "opacity": 0,
        "duration": 0.8,
        "stagger": 0.15,
        "delay": 0.5,
        "ease": "power3.out"
      },
      "hover": {
        "title": {
          "color": "#0066FF",
          "duration": 0.5,
          "ease": "power1.inOut"
        },
        "image": {
          "scale": 1.08,
          "duration": 0.8,
          "ease": "power1.out"
        },
        "link": {
          "x": 5,
          "duration": 0.5,
          "ease": "back.out(1.2)"
        }
      },
      "category": {
        "y": -5,
        "duration": 3,
        "ease": "sine.inOut",
        "repeat": -1,
        "yoyo": true,
        "delay": "random(0.8)"
      }
    },
    "cta": {
      "container": {
        "y": 30,
        "opacity": 0,
        "duration": 0.8,
        "delay": 0.6,
        "ease": "power3.out"
      },
      "decorativeElements": {
        "scale": 0.7,
        "opacity": 0,
        "duration": 0.7,
        "stagger": 0.2,
        "delay": 0.7,
        "ease": "power2.out"
      },
      "title": {
        "y": 20,
        "opacity": 0,
        "duration": 0.7,
        "delay": 0.8,
        "ease": "power2.out"
      },
      "description": {
        "y": 20,
        "opacity": 0,
        "duration": 0.7,
        "delay": 0.9,
        "ease": "power2.out"
      },
      "buttons": {
        "y": 20,
        "opacity": 0,
        "duration": 0.7,
        "stagger": 0.2,
        "delay": 1.0,
        "ease": "back.out(1.2)"
      },
      "floating": {
        "y": -15,
        "x": 8,
        "duration": 8,
        "ease": "sine.inOut",
        "repeat": -1,
        "yoyo": true,
        "stagger": 3
      }
    }
  }
}
