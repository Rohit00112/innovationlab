  "calendarInfo": {
    "containerClasses": "w-full md:w-1/3",
    "title": {
      "text": "Events Calendar",
      "classes": "text-3xl md:text-4xl font-bold mb-4"
    },
    "description": {
      "text": "Stay up-to-date with our upcoming events. Add them to your calendar and never miss an opportunity to learn and connect.",
      "classes": "text-base text-gray-600 mb-6"
    },
    "categoriesBox": {
      "containerClasses": "bg-white rounded-xl p-6 shadow-md border border-gray-100",
      "title": {
        "text": "Event Categories",
        "classes": "text-lg font-bold text-gray-900 mb-4"
      },
      "categories": [
        {
          "color": "bg-[#0055D4]",
          "colorClasses": "inline-block w-3 h-3 rounded-full",
          "text": "Conferences & Summits",
          "textClasses": "text-sm text-gray-800"
        },
        {
          "color": "bg-[#4538D9]",
          "colorClasses": "inline-block w-3 h-3 rounded-full",
          "text": "Workshops & Training",
          "textClasses": "text-sm text-gray-800"
        },
        {
          "color": "bg-[#D99B22]",
          "colorClasses": "inline-block w-3 h-3 rounded-full",
          "text": "Competitions & Networking",
          "textClasses": "text-sm text-gray-800"
        },
        {
          "color": "bg-[#00A344]",
          "colorClasses": "inline-block w-3 h-3 rounded-full",
          "text": "Forums & Seminars",
          "textClasses": "text-sm text-gray-800"
        }
      ],
      "footer": {
        "containerClasses": "mt-6 pt-6 border-t border-gray-100",
        "link": {
          "href": "/events/calendar",
          "classes": "inline-flex items-center justify-center w-full px-4 py-2 bg-[#0055D4] text-white text-sm font-medium rounded-md hover:bg-[#004BBB] transition-colors",
          "text": "View Full Calendar"
        }
      }
    }
  },
  "calendarEvents": {
    "containerClasses": "w-full md:w-2/3",
    "box": {
      "containerClasses": "bg-white rounded-xl p-6 shadow-md border border-gray-100",
      "header": {
        "containerClasses": "flex items-center justify-between mb-6",
        "title": {
          "text": "Upcoming Events",
          "classes": "text-lg font-bold text-gray-900"
        },
        "navigation": {
          "containerClasses": "flex items-center gap-2",
          "prevButton": {
            "classes": "p-1 rounded hover:bg-gray-100",
            "ariaLabel": "Previous month",
            "icon": {
              "width": "20",
              "height": "20",
              "viewBox": "0 0 24 24",
              "path": {
                "d": "M15 19L8 12L15 5",
                "stroke": "#1F2937",
                "strokeWidth": "2",
                "strokeLinecap": "round",
                "strokeLinejoin": "round"
              }
            }
          },
          "dateRange": {
            "text": "November - December 2023",
            "classes": "text-sm font-medium text-gray-800"
          },
          "nextButton": {
            "classes": "p-1 rounded hover:bg-gray-100",
            "ariaLabel": "Next month",
            "icon": {
              "width": "20",
              "height": "20",
              "viewBox": "0 0 24 24",
              "path": {
                "d": "M9 5L16 12L9 19",
                "stroke": "#1F2937",
                "strokeWidth": "2",
                "strokeLinecap": "round",
                "strokeLinejoin": "round"
              }
            }
          }
        }
      },
      "eventsList": {
        "containerClasses": "space-y-1 divide-y divide-gray-100"
      }
    }
  },
  "eventCard": {
    "containerClasses": "flex items-start gap-4 p-4 rounded-lg hover:bg-gray-50 transition-colors duration-300",
    "dateBox": {
      "containerClasses": "flex flex-col items-center justify-center min-w-16 h-16 bg-white rounded-lg shadow-sm border border-gray-100 overflow-hidden",
      "month": {
        "classes": "text-xs font-medium text-gray-600"
      },
      "day": {
        "classes": "text-xl font-bold text-gray-900"
      },
      "year": {
        "classes": "text-xs font-medium text-gray-600"
      }
    },
    "content": {
      "containerClasses": "flex-1",
      "category": {
        "containerClasses": "flex items-center gap-2 mb-1",
        "indicator": {
          "classes": "inline-block w-2 h-2 rounded-full"
        },
        "text": {
          "classes": "text-xs font-medium text-gray-700"
        }
      },
      "title": {
        "classes": "text-base font-semibold text-gray-900 group-hover:text-[#0055D4] transition-colors duration-300 mb-1"
      },
      "time": {
        "containerClasses": "flex items-center gap-1 text-xs text-gray-700",
        "icon": {
          "width": "12",
          "height": "12",
          "viewBox": "0 0 24 24",
